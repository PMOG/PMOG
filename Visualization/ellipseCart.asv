function [ ] = ellipseCart( )
%ELLIPSECART Summary of this function goes here
%   Detailed explanation goes here
M=512;
a=1;
b=2;
shift=pi/3;

N=128;
A=zeros(N,N);
L=sqrt(a^2+b^2);

th=linspace(0,2*pi,M);

xt=a/L*cos(th);
yt=b/L*sin(th);
% Rotate
xr=cos(shift)*xt+sin(shift)*yt;
yr=sin(shift)*xt-cos(shift)*yt;
% Index
xi=floor(N*(xr+1)/2);
yi=floor(N*(yr+1)/2);
idx=yi*N+xi+1;

A(idx)=1;
imagesc(A);
end